{
    "manifest_version": 2,

    "name": "History Vis",
    "description": "This extension aims to help visualize your chrome history",
    "version": "1.0",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "index.html"
    },
    "background": {
        "scripts": []
    },
    "permissions": [
        "activeTab",
        "storage",
        "history", 
        "clipboardRead",
        "clipboardWrite",
        "geolocation"
    ]
}
